	<insert id="fallbackinsert" parameterType="com.slloan.entity.CircuLationRecord">  
 	insert into recordsubmit_table 
(
		
		fallbackname ,
		submit ,
		state,
		spare1 ,
		createDate ,
		username,
		parentnodeId,
		city,
		rolename
		)
		VALUES
		(
		#{fallbackname},
		#{submit},
		#{state},
		#{spare1},
		#{createDate},
		#{username},
		#{ParentnodeId},
		#{city},
		#{rolename})
    </insert>
